<%= form_for @newrecipe, url: "/recipes" do |f| %>
  Name: <%= f.text_field :name %><br>
  Difficulty: <%= f.text_field :difficulty %><br>
  Cooking Time: <%= f.text_field :cookingtime_seconds %><br>
  Author: <%= f.text_field :author %><br>
  Price: <%= f.text_field :price_cents %><br>
  Ingredients: <%= f.text_field :ingredients %><br>
  Photo URL: <%= f.text_field :photo %><br>
  <%= f.submit %>
<% end %>
